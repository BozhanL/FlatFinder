{
  "name": "flatfinder",
  "main": "src/index.ts",
  "version": "1.0.0",
  "homepage": "https://github.com/BozhanL/FlatFinder",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BozhanL/FlatFinder.git"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/BozhanL/FlatFinder/issues"
  },
  "scripts": {
    "postinstall": "tsx scripts/fix-lib-types.ts",
    "prepush": "npm run check-ts && npm run lint && npm run format && npm run coverage",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:android": "expo prebuild --platform android",
    "prebuild:android:clean": "expo prebuild --platform android --clean",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "check-ts": "tsc",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "deploy": "firebase deploy",
    "deploy:database": "firebase deploy --only database",
    "deploy:firestore": "firebase deploy --only firestore"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@notifee)"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary",
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "./src/**/*.{ts,tsx,js,jsx}"
    ],
    "coverageDirectory": "coverage"
  },
  "engines": {
    "node": "^22",
    "npm": "^10"
  },
  "dependencies": {
    "@expo-google-fonts/playfair-display": "^0.4.1",
    "@expo-google-fonts/poppins": "^0.4.0",
    "@expo-google-fonts/roboto": "^0.4.0",
    "@expo/vector-icons": "^14.1.0",
    "@maplibre/maplibre-react-native": "^10.2.1",
    "@notifee/react-native": "^9.1.8",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-native-firebase/app": "^23.2.0",
    "@react-native-firebase/auth": "^23.2.0",
    "@react-native-firebase/database": "^23.2.0",
    "@react-native-firebase/firestore": "^23.2.0",
    "@react-native-firebase/messaging": "^23.2.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.75.0",
    "base64-arraybuffer": "^1.0.2",
    "dayjs": "^1.11.18",
    "expo": "53.0.22",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.6",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "lodash": "^4.17.21",
    "mime": "^4.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-gifted-chat": "^2.8.1",
    "react-native-keyboard-controller": "^1.18.6",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-swiper-flatlist": "^3.2.5",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "react-navigation-header-buttons": "^13.0.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.36.0",
    "@testing-library/react-native": "^13.3.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "~19.0.10",
    "@types/yup": "^0.29.14",
    "eslint": "^9.36.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-testing-library": "^7.6.6",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "prettier": "^3.6.2",
    "pretty-format": "^29.7.0",
    "react-test-renderer": "~19.0.0",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.45.0"
  },
  "private": true
}
